      <form class="form-signin">
        <h2 class="form-signin-heading">ATC Career application form</h2>
        <div class="form-group">
            <label for="email">Email address:</label>
            <input type="email" class="form-control" id="email">
        </div>
        <div class="form-group">
            <label for="inputCID">VATSIM CID</label>
            <input type="text" id="inputCID" class="form-control" placeholder="VATSIM CID" required autofocus>
        </div>
        <div class="form-group">
            <label for="inputEmail">Email address</label>
            <input type="email" id="inputEmail" class="form-control" placeholder="Email address" required>
        </div>
        <div class="form-group">
            <label for="inputDivision">VATSIM Division</label>
            <input type="text" id="inputDivision" class="form-control" placeholder="eg. VATEUD" required>
        </div>
        <div class="form-group">
            <label for="inputVACC">VATSIM VACC</label>
            <input type="text" id="inputVACC" class="form-control" placeholder="eg. Romania" required>
        </div>
        <button class="btn btn-lg btn-primary btn-block" type="submit">Submit</button>
      </form>

